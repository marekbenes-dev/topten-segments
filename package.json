{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "lint": "eslint",
    "format:check": "prettier --check .",
    "build:verify": "NEXT_TELEMETRY_DISABLED=1 next build --no-lint",
    "prepare": "git config core.hooksPath .githooks || true"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,css,scss}": "prettier --write"
  },
  "dependencies": {
    "cookies-next": "^6.1.0",
    "jsonwebtoken": "^9.0.2",
    "leaflet": "^1.9.4",
    "next": "15.5.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-leaflet": "^5.0.0",
    "strava-v3": "^2.2.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/leaflet": "^1.9.20",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9.36.0",
    "eslint-config-next": "15.5.3",
    "lint-staged": "^16.2.0",
    "prettier": "^3.6.2",
    "tailwindcss": "^4",
    "typescript": "^5.9.2"
  }
}
